{
    "version": "0.1.0",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/jeremiah-olisa/node-utils.git"
    },
    "engines": {
        "node": ">=14"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "peerDependencies": {},
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "node-express-utils",
    "author": "Jeremiah Olisa",
    "module": "dist/node-utils.esm.js",
    "size-limit": [{
            "path": "dist/node-utils.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/node-utils.esm.js",
            "limit": "10 KB"
        }
    ],
    "dependencies": {
        "cookie-parser": "^1.4.6",
        "express-mongo-sanitize": "^2.2.0",
        "g": "^2.0.1",
        "helmet": "^5.0.2",
        "hpp": "^0.2.3",
        "mongoose": "^6.3.0",
        "morgan": "^1.10.0",
        "tsdx": "^0.14.1",
        "xss-clean": "^0.1.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/mongoose": "^5.11.97",
        "@types/cookie-parser": "^1.4.2",
        "@types/hpp": "^0.2.2",
        "@types/morgan": "^1.9.3"
    }
}